{{#with heading}}
  <div class='card'>
    <div class='card-body'>
      {{#with main}}
        <div class='card'>
          <div class='card-body'>
            <div class='display-1'>{{title}}</div>
            <p>{{text}}</p>
            {{#with button_text}}
              <button class='btn btn-primary'>
                {{solid}}
              </button>
              <br />
              <br />
              <p>
                <button class='btn btn-outline-primary'>
                  {{outline}}
                </button>
              </p>
            {{/with}}
          </div>
        </div>
      {{/with}}
      <br />
      {{#with second}}
        <div class='card'>
          <div class='card-body'>
            <div class='display-2'> {{title}} </div>
            <p>{{text}}</p>
            {{#with button_text}}
              <button class='btn btn-secondary'>
                {{solid}}
              </button>
              <br />
              <br />
              <p>
                <button class='btn btn-outline-secondary'>
                  {{outline}}
                </button>
              </p>
            {{/with}}
          </div>
        </div>
      {{/with}}
      <br />
      {{#with subblock}}
        <div class='card'>
          <div class='card-body'>
            <div class='display-3'> {{title}} </div>
            <p>{{text}}</p>
            {{#with button_text}}
              <button class='btn btn-dark'>
                {{solid}}
              </button>
              <br />
              <br />
              <p>
                <button class='btn btn-outline-dark'>
                  {{outline}}
                </button>
              </p>
            {{/with}}
          </div>
        </div>
      {{/with}}
    </div>
  </div>
{{/with}}
<br />
<br />
{{#with nested_display}}
  <div class='card'>
    <div class='card-body'>
      <div class='display-1'> {{title}} </div>
      <div class='card'>
        <br>
        {{#with children_display}}
          <div class='card-body'>
            <div class='display-4'> {{title}} </div>
            <p> {{text}} </p>
            <button class="btn btn-outline-warning"> {{button.text}} </button>
          </div>
        </div>
        {{/with}}
        <br>
        <div class="display-6">{{header_text}}</div>
        <br>
        <button class="btn btn-primary"> {{button.text}} </button>
    </div>
  </div>
{{/with}}
<br>
<br>
<div class="card">
  <div class="card-body">
    {{#with paragraph}}
      <p class="text-uppercase"> {{title}} </p>
      <p class="text-lowercase"> {{text}} </p>
      <a href="{{button.url}}" class="btn btn-link"> {{button.text}} </a>
      <div class="card">
          <div class="card-body">
            {{#each list_name}}
            <ul>
              <li>
              <a href="{{url}}" class="btn btn-link"> {{text}} </a>
              </li>
            </ul>             
            {{/each}}
      </div>
        </div>
    {{/with}}
  </div>
</div>